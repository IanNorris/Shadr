<ShaderSyntax IndentType="Tabs" NewlineType="Windows" Buckets="Header;Prototypes;Globals;Functions">
	<Program>
		<Iterate Target="Elements">
			<PrintChild/>
		</Iterate>
	</Program>
	<Block>
		<Newline/>
		<Print Literal="{"/>
		<Newline/>
		<AddIndent/>
		<Iterate Target="Statements">
			<PrintChild/>
			<SemiColon/>
			<InsertOriginalNewlines Min="1"/>
		</Iterate>
		<RemoveIndent/>
		<Print Literal="}"/>
	</Block>
	
	<ConstFloat>
		<Print Value="m_fValue"/>
		<Print Literal="f"/>
	</ConstFloat>
</ShaderSyntax>
